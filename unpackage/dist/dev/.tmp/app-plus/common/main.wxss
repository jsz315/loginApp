.status_bar {
		height: var(--status-bar-height);
		background: #682FE9;
		width: 100%;
}
.jbtn{
		width: 690rpx;
		height: 96rpx;
		line-height: 96rpx;
		background: #682FE9;
		color: #FFFFFF;
		text-align: center;
		margin: 20rpx auto;
		font-size: 32rpx;
		border-radius: 8rpx;
}
.jarrow{
		width: 30rpx;
		height: 30rpx;
		margin-right: 30rpx;
		background: url(data:image/png;base64,) no-repeat center;
		background-size: contain;
}
.jrow-box{
		padding: 20rpx;
		border-top: solid 1rpx #ededed;
		border-bottom: solid 1rpx #ededed;
		background: #FFFFFF;
		position: relative;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
}
.jrow-input{
		-webkit-box-flex: 1;
		-webkit-flex: 1;
		        flex: 1;
		text-align: left;
		display: block;
		box-sizing: border-box;
		padding: 0 20rpx;
		font-size: 32rpx;
		color: #999999;
		height: 60rpx;
		line-height: 60rpx;
		overflow: hidden;
		text-overflow: ellipsis;
		white-space: nowrap;
}
	/* 滑动组件样式---------------- */
